{
     "schemaVersion": 1,
     "id": "cmd-utils",
     "version": "${version}",
     "name": "CMD Utils",
     "description": "A FabricMC Minecraft modification used for glitch hunting.",

     "authors": [
          "Coderx_Gamer"
     ],

     "contact": {
          "homepage": "https://github.com/Coderx-Gamer/cmd-utils/",
          "sources": "https://github.com/Coderx-Gamer/cmd-utils/"
     },

     "license": "GPL-3.0",
     "icon": "assets/cmd-utils/icon.png",
     "environment": "client",
     "entrypoints": {
          "client": [
               "org.cmdutils.MainClient"
          ]
     },

     "mixins": [
          "cmd-utils.mixins.json"
     ],

     "accessWidener": "cmd-utils.accesswidener",

     "depends": {
          "fabricloader": ">=0.14.24",
          "minecraft": "~1.20.2",
          "java": ">=17",
          "fabric-api": "*"
	 },

     "suggests": {
          "another-mod": "*"
     },

     "custom": {
          "modmenu:clientsideOnly": true,
          "modmenu:api": false
     }
}
